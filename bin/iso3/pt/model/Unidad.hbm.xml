<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="iso3.pt.model.Unidad" table="UNIDAD">

        <id name="Id" column="UNID_ID">
            <generator class="native"/>
        </id>

        <property name="Acronimo" column="UNID_ACRONIMO" unique="true" not-null="true"/>
        <property name="Titulo" column="UNID_TITULO" not-null="true"/>
        <property name="Contenido" column="UNID_CONTENIDO"/>
       
		
		

    </class>

</hibernate-mapping>